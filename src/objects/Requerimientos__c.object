    <validationRules>
        <fullName>RQ001_Solicitud_de_credito</fullName>
        <active>true</active>
        <description>Valida que se diligencien los campos consecutivo de la solicitud y número de documento cuando se indica en el campo módulo de incidencia &quot;Solicitud de crédito&quot;.</description>
        <errorConditionFormula>(ISPICKVAL( Modulo_de_incidencia__c , &quot;Solicitud de crédito&quot;)&amp;&amp;

( ISBLANK( Consecutivo_de_la_solicitud__c )  ||
 ISBLANK( Observacion__c )  ||
 ISBLANK( Numero_documento__c ) ))</errorConditionFormula>
        <errorMessage>RQ-001.Cuando indica en el campo módulo de incidencia: &quot;Solicitud de crédito&quot;, debe diligenciar los campos consecutivo de la solicitud, número de documento y observación.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RQ002_Candidatos</fullName>
        <active>true</active>
        <description>Valida que se diligencien los campos número de documento y observación cuando se indica en el campo módulo de incidencia &quot;Candidatos&quot;.</description>
        <errorConditionFormula>AND(
 ISPICKVAL(Modulo_de_incidencia__c, &quot;Candidatos&quot;) ,
 OR(
 ISBLANK(Numero_documento__c) ,
 ISBLANK(Observacion__c) 

) )</errorConditionFormula>
        <errorMessage>RQ-002.Cuando indica en el campo módulo de incidencia: &quot;Candidatos&quot;, debe diligenciar los campos número de documento y observación.</errorMessage>
