    <validationRules>
        <fullName>C001_Valida_cliente_deudor</fullName>
        <active>true</active>
        <errorConditionFormula>ISCHANGED( Cliente_deudor__c )</errorConditionFormula>
        <errorDisplayField>Cliente_deudor__c</errorDisplayField>
        <errorMessage>C001-No modificar el cliente deudor</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>C002_Consecutivo_de_la_radicacion</fullName>
        <active>true</active>
        <errorConditionFormula>ISCHANGED( Radicacion__c )</errorConditionFormula>
        <errorDisplayField>Radicacion__c</errorDisplayField>
        <errorMessage>C002-No modificar el consecutivo de la radicación.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>C003_Consecutivo_de_la_solicitud</fullName>
        <active>true</active>
        <errorConditionFormula>ISCHANGED(  Solicitud_de_credito__c )</errorConditionFormula>
        <errorMessage>C003-No modificar el consecutivo de la solicitud.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>C004_Valida_relacion_cuota_ingreso</fullName>
        <active>true</active>
        <errorConditionFormula>AND(

ISPICKVAL(El_credito_fue__c ,&quot;Preaprobado&quot;),

Relacion_cuota_ingreso_propuesta__c &gt;= 0.3 


)</errorConditionFormula>
        <errorMessage>C004-La relación cuota ingreso es mayor ó igual al 30% , el crédito no pueder aprobado.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>C005_Valida_endeudamiento_financiero</fullName>
        <active>true</active>
        <errorConditionFormula>AND(

ISPICKVAL(El_credito_fue__c ,&quot;Preaprobado&quot;),

Endeudamiento_financiero_propuesta__c &gt;= 0.5 

)</errorConditionFormula>
        <errorMessage>C005-El endeudamiento financiero  es mayor ó igual al 50% , el crédito no puede ser aprobado.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>C006_Valida_endeudamiento_financiero2</fullName>
        <active>true</active>
        <description>Valida endeudamiento financiero más cuota</description>
        <errorConditionFormula>AND(

ISPICKVAL(El_credito_fue__c ,&quot;Preaprobado&quot;),

Endeudamiento_financiero_cuota_propuesta__c  &gt;= 0.6 ,
Solicitud_de_credito__r.Migrado__c = FALSE


)</errorConditionFormula>
        <errorMessage>C006-El endeudamiento financiero más cuota  es mayor ó igual al 60% , el crédito no puede ser aprobado.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>C007_Valida_indice_cuota_ingreso_disponi</fullName>
        <active>true</active>
        <errorConditionFormula>AND(

ISPICKVAL(El_credito_fue__c ,&quot;Preaprobado&quot;),

Ind_relacion_cuota_ingeso_neto_propuesta__c  &gt; 1 



)</errorConditionFormula>
        <errorMessage>C007-El indice relación cuota ingreso  es mayor ó igual al 100% , el crédito no puede ser aprobado.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>C008_Valida_LTV_sugerido</fullName>
        <active>false</active>
        <errorConditionFormula>AND(

ISPICKVAL(El_credito_fue__c ,&quot;Preaprobado&quot;),

Ltv_sugerido__c &gt; 0.7 ,
Solicitud_de_credito__r.Migrado__c = FALSE


)</errorConditionFormula>
        <errorMessage>C008-El LTV sugerido es mayor ó igual  al 70% , el crédito no puede ser aprobado.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>C009_Valida_credito_solicitado</fullName>
        <active>false</active>
        <errorConditionFormula>AND(

ISPICKVAL(El_credito_fue__c ,&quot;Preaprobado&quot;),

Credito_sugerido__c  &lt;  6894500 


)</errorConditionFormula>
        <errorDisplayField>Aprobado_con_programa_MI_CASA_YA__c</errorDisplayField>
        <errorMessage>C009-El crédito sugerido debe ser mayor a $6.894.500 , el crédito no puede ser aprobado.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>C010_Valida_score_del_cliente_deudor</fullName>
        <active>true</active>
        <errorConditionFormula>AND(

ISPICKVAL(El_credito_fue__c ,&quot;Preaprobado&quot;),

Score__c  &lt; 200 


)</errorConditionFormula>
        <errorMessage>C010-El score del cliente deudor debe ser mayor de 200, el crédito no puede ser aprobado.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>C011_Valida_acierta_del_cliente_deudor</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL(El_credito_fue__c ,&quot;Preaprobado&quot;) &amp;&amp;    Acierta__c  &lt;&gt; 0   &amp;&amp;   Acierta__c  &lt;&gt; 5 &amp;&amp;   Acierta__c  &lt;&gt; 7  &amp;&amp;   Acierta__c  &lt; 606</errorConditionFormula>
        <errorMessage>C011-El acierta del cliente deudor debe ser mayor de 606, el crédito no puede ser aprobado.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>C012_Valida_ingresos_mes_del_deudor</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
DATEVALUE(CreatedDate) &gt;= DATEVALUE(&quot;2017-01-01&quot;), 
ISPICKVAL(El_credito_fue__c ,&quot;Preaprobado&quot;),

OR(
Ingresos_mes__c &lt; 737717

))</errorConditionFormula>
        <errorMessage>C012-El ingreso mes del cliente deudor debe ser mayor de $737.717 , el crédito no puede ser aprobado.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>C013_Valida_antiguedad_en_meses</fullName>
        <active>true</active>
        <errorConditionFormula>AND(

ISPICKVAL(El_credito_fue__c ,&quot;Preaprobado&quot;),

Antig_edad_en_meses__c  &lt;  3 


)</errorConditionFormula>
        <errorMessage>C013-La antigüedad laboral del cliente deudor debe ser mayor a 3 meses ,el crédito no puede ser aprobado.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>C014_Valida_el_plazo</fullName>
        <active>true</active>
        <errorConditionFormula>AND(

ISPICKVAL(El_credito_fue__c ,&quot;Preaprobado&quot;),

OR(

Plazo_propuesta__c  &gt;  180,
Plazo_propuesta__c  &lt; 60

))</errorConditionFormula>
        <errorMessage>C014-El plazo debe estar entre 60 - 180, el crédito no puede ser aprobado.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>C015_Valida_causales_de_negacion</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
ISPICKVAL( El_credito_fue__c ,&quot;Negado&quot;), 

ISPICKVAL( Causal_de_negacion_INCO__c ,&quot;&quot;) 


)</errorConditionFormula>
        <errorMessage>C015- Si el crédito se encuentra negado, por favor diligenciar el causal de negación</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>C016_Valida_credito_desistido</fullName>
        <active>true</active>
        <errorConditionFormula>AND(

ISPICKVAL(  Credito_desistido__c   ,&quot;Si&quot;), 
ISBLANK( Fecha_de_desistido__c ) 


)</errorConditionFormula>
        <errorDisplayField>Fecha_de_desistido__c</errorDisplayField>
        <errorMessage>C016- Si el crédito está desistido, por favor diligenciar la fecha</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>C017_Valida_credito_sugerido</fullName>
        <active>true</active>
        <errorConditionFormula>AND(

ISPICKVAL(El_credito_fue__c ,&quot;Preaprobado&quot;),

Credito_solicitado__c  &lt; 6894500 


