    <validationRules>
        <fullName>OP111_Proyecto_bloqueado</fullName>
        <active>false</active>
        <description>No permite relacionar al proyecto Caminos de Sie.</description>
        <errorConditionFormula>AND( 


Proyecto__c  = &quot;a08o0000004BHqw&quot; , 

OR( 
ISPICKVAL( Tipo_de_producto__c ,&quot;Crédito de vivienda nueva&quot;), 
ISPICKVAL( Tipo_de_producto__c ,&quot;Leasing habitacional vivienda nueva&quot;),
ISPICKVAL( Tipo_de_producto__c ,&quot;Asignación de cupo&quot;),
ISPICKVAL( Tipo_de_producto__c ,&quot;Crédito vivienda usada&quot;),
ISPICKVAL( Tipo_de_producto__c ,&quot;Leasing habitacional vivienda usada&quot;),
ISPICKVAL( Tipo_de_producto__c ,&quot;Mejoramiento de vivienda&quot;) 
))</errorConditionFormula>
        <errorMessage>OP-000. El proyecto se encuentra detenido..</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OP111_Valida_el_boton_guardar_en_la_opor</fullName>
        <active>false</active>
        <description>Valida que no se pueda modificar la oportunidad si el estado de la solicitud no está en Nueva solicitud, Reproceso , Reproceso devuelta,Devuelta,Negada,Legalizado sin constructora y proyecto,Desistida  y aplazada.</description>
        <errorConditionFormula>AND( NOT( ISNEW() ),

$Profile.Id  &lt;&gt; &quot;00eo0000000f7LO&quot;,
$Profile.Id  &lt;&gt; &quot;00eo0000000f645&quot;,
$Profile.Id  &lt;&gt; &quot;00eo0000000TUb7&quot;,

ISPICKVAL( Solicitud_de_credito__r.Estado_de_la_solicitud__c , &quot;Nueva Solicitud&quot;),

ISPICKVAL( Solicitud_de_credito__r.Estado_de_la_solicitud__c , &quot;Reproceso&quot;),

ISPICKVAL( Solicitud_de_credito__r.Estado_de_la_solicitud__c , &quot;Reproceso devuelto&quot;),

ISPICKVAL( Solicitud_de_credito__r.Estado_de_la_solicitud__c ,&quot;Devuelta&quot; ),

ISPICKVAL( Solicitud_de_credito__r.Estado_de_la_solicitud__c ,&quot;Negada&quot; ),

ISPICKVAL(Solicitud_de_credito__r.Estado_de_la_solicitud__c ,&quot;Legalizado sin constructora y proyecto&quot;),
   
ISPICKVAL(Solicitud_de_credito__r.Estado_de_la_solicitud__c ,&quot;Desistida&quot;),

ISPICKVAL( Solicitud_de_credito__r.Estado_de_la_solicitud__c , &quot;Aplazada&quot;),

ISPICKVAL( Solicitud_de_credito__r.Estado_de_la_solicitud__c , &quot;Carta definitiva no generada&quot;)

)</errorConditionFormula>
        <errorMessage>OP-000. No puede modificar la oportunidad si no está en los estados de la solicitud : Nueva Solicitud, Reproceso, Reproceso devuelto, Devuelta, Negada, Legalizado sin constructora y proyecto, Desistida, Aplazada y Carta definitiva no generada.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OP111_Valida_etapa_ganada</fullName>
        <active>false</active>
        <description>Cuando el resultado del prevalidador es igual a &quot;NO VIABLE&quot;, no permite cambiar la etapa de la oportunidad a Abierta.</description>
        <errorConditionFormula>AND(
ISPICKVAL(StageName,&quot;Ganada&quot;),

OR(
Resultado_del_Prevalidador__c =&quot;NO VIABLE&quot;

))</errorConditionFormula>
        <errorMessage>OP000. Si el cliente es &quot;NO VIABLE &quot;, no indicar en la etapa de la oportunidad como &quot;Ganada&quot;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OP111_Valida_numero_de_oportunidades</fullName>
        <active>false</active>
        <description>Valida que solamente se pueda crear una oportunidad por tipo de producto.</description>
        <errorConditionFormula>ISNEW() &amp;&amp;

1 &lt;= Account.Tipo_de_pcto_asignacion_de_cupo__c  &amp;&amp; 
1 &lt;= Account.Tipo_de_pcto_credito_de_vivienda_nueva__c &amp;&amp;
1 &lt;= Account.Tipo_de_pcto_credito_de_vivienda_usada__c &amp;&amp;
1 &lt;= Account.Tipo_de_pcto_leasing_vivienda_nueva__c  &amp;&amp;
1 &lt;= Account.Tipo_de_pcto_mejoramiento_de_vivienda__c &amp;&amp;
1 &lt;= Account.Tipo_de_pcto_leasing_vivienda_usada__c &amp;&amp;
1 &lt;= Account.Tipo_de_pcto_cuenta_de_ahorros__c &amp;&amp;
1 &lt;= Account.Tipo_de_pcto_CDT__c</errorConditionFormula>
        <errorMessage>OP-000.Solamente es permitido crear una oportunidad por tipo de producto.</errorMessage>
    </validationRules>
