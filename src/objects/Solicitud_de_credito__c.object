    <validationRules>
        <fullName>S001_Registro_checklist_de_solicitud</fullName>
        <active>true</active>
        <description>Cuando una solicitud de crédito está en estado radicada se debe diligenciar toda la sección: Checklist de documentos para solicitud de créditos.</description>
        <errorConditionFormula>AND( 

RecordType.Name =&quot;Vivienda Nueva&quot;,
RecordType.Name =&quot;Vivienda Usada / Mejoramiento&quot;,
ISPICKVAL(Estado_de_la_solicitud__c,&quot;Radicada&quot;),

OR(
ISPICKVAL( Cliente_con_visita__c,&quot;&quot;),
ISPICKVAL( Formato_de_vinculacion_diligenciado__c,&quot;&quot;),
ISPICKVAL( fotocopia_c_dula_ampliada_al_150__c,&quot;&quot;),
ISPICKVAL( CIFIN_en_carpeta_virtual__c,&quot;&quot;),
ISPICKVAL( Data_en_carpeta_virtual__c,&quot;&quot;),
ISPICKVAL( Paz_y_salvo_de_moras_embargos__c,&quot;&quot;),
ISPICKVAL( Certificado_de_ingresos_y_retenciones_de__c,&quot;&quot;),
ISPICKVAL( Solicitud_con_frech__c,&quot;&quot;),
ISPICKVAL( Certificado_vinculacion_a_empresa_transp__c,&quot;&quot;),
ISPICKVAL( Estados_financieros_2_ultim__c,&quot;&quot;),
ISPICKVAL( Contratos_de_arrendamientos__c,&quot;&quot;),
ISPICKVAL( Certificado_tradicion_y_libertad__c,&quot;&quot;),
ISPICKVAL( Certificado_de_otros_ingresos__c,&quot;&quot;),
ISPICKVAL( Referencias_de_proveedores__c,&quot;&quot;),
ISPICKVAL( Carta_laboral_con_especificaciones__c,&quot;&quot;),
ISPICKVAL( Documento_afiliacion_al_SGSS_salud_pensi__c,&quot;&quot;),
ISPICKVAL( Comprobante_pago_pension_ultimos_2_meses__c,&quot;&quot;),
ISPICKVAL( Comprobantes_de_nomina_ultimos_2_meses__c,&quot;&quot;),
ISPICKVAL( Certificado_de_camara_de_comercio__c,&quot;&quot;),
ISPICKVAL( Registro_unico_tributario__c,&quot;&quot;),
ISPICKVAL( Extractos_bancarios_ultimos_tres_meses__c,&quot;&quot;),
ISPICKVAL( Documento_de_afiliacion_arp__c,&quot;&quot;),
ISPICKVAL( Documento_afiliac_caja_de_compensacion__c,&quot;&quot;),
ISPICKVAL( Historia_laboral_de_fondo_de_pensiones__c,&quot;&quot;),
ISPICKVAL( Recibo_de_pago_de_visita_a_independiente__c,&quot;&quot;),
ISPICKVAL( Carta_de_cliente_con_frech__c,&quot;&quot;),
ISPICKVAL( Declaracion_renta_o_carta_no_declarante__c,&quot;&quot;),
ISPICKVAL( Fotocopia_tarjeta_de_propiedad_vehiculo__c,&quot;&quot;),
ISPICKVAL( Extracto_bancario_factura_inscripcion_TV__c,&quot;&quot;),
