    <validationRules>
        <fullName>CD001_Valida_documento_numerico</fullName>
        <active>true</active>
        <description>Valida que el número de documento sea de tipo numérico</description>
        <errorConditionFormula>NOT ISNUMBER( Numero_de_identificacion__c)</errorConditionFormula>
        <errorDisplayField>Numero_de_identificacion__c</errorDisplayField>
        <errorMessage>CD-001. Este campo solo acepta caracteres numéricos.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CD002_Valida_candidato_NO_VIABLE</fullName>
        <active>true</active>
        <description>Si el candidato tiene un puntaje de datacredito mayor a 657 es &quot;NO VIABLE&quot;.</description>
        <errorConditionFormula>AND(

ISPICKVAL( Concepto_del_candidato__c ,&quot; NO VIABLEe&quot; ),
Puntaje_data_credito__c &gt;= 657

)</errorConditionFormula>
        <errorMessage>CD-002. Candidato &quot;NO VIABLE&quot; por puntaje datacredito.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CD003_Cliente_no_prevalidado</fullName>
        <active>true</active>
        <description>Los candidatos que residan en Bogotá o Medellín que tengan un puntaje de datacredito meno o igual a 658.</description>
        <errorConditionFormula>AND(

ISPICKVAL(Concepto_del_candidato__c ,&quot;No prevalidado&quot; ),
Puntaje_data_credito__c &lt;&gt; 0

)</errorConditionFormula>
        <errorMessage>CD-003. Si el candidato &quot;No es prevalidado&quot; no debe tener puntaje data crédito igual a cero.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CD004_Valida_candidato_VIABLE</fullName>
        <active>true</active>
        <description>Los candidatos que residan en Bogotá o Medellín que tengan un puntaje de datacredito meno o igual a 658.</description>
        <errorConditionFormula>AND(

ISPICKVAL( Concepto_del_candidato__c ,&quot;Viable&quot; ),
Puntaje_data_credito__c &lt;= 658,

ISPICKVAL( Ciudad_de_residencia__c , &quot;Bogotá&quot;) ,
ISPICKVAL( Ciudad_de_residencia__c , &quot;Medellín&quot;)

)</errorConditionFormula>
        <errorMessage>CD-004. Candidato &quot;VIABLE&quot;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CD005_No_permite_convertir_el_candidato</fullName>
        <active>true</active>
        <description>No convertir el candidato si el candidato es no viable y no prevalidado</description>
        <errorConditionFormula>AND( IsConverted   = TRUE,


OR(
ISPICKVAL(Concepto_del_candidato__c ,&quot;NO VIABLE&quot;),
ISPICKVAL(Concepto_del_candidato__c ,&quot;No prevalidado&quot;)


))</errorConditionFormula>
        <errorMessage>CD-005. El candidato es &quot;NO VIABLE&quot; o &quot;NO PREVALIDADO &quot; por favor no convertir.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CD006_No_modificar_estado_del_candidato</fullName>
        <active>true</active>
        <description>No convertir el candidato si se encuentra en el estado del candidato : Nuevo,Descartado,Verificando,Asignado</description>
        <errorConditionFormula>AND( 
    $User.ProfileId &lt;&gt; &quot;00eo0000000uvB5&quot;, 
    $User.ProfileId &lt;&gt; &quot;00eo0000000TUaz&quot;,

OR(
    ISPICKVAL( Status ,&quot;Nuevo&quot;),
    ISPICKVAL( Status ,&quot;Verificando&quot;),
    ISPICKVAL( Status ,&quot;Descartado&quot;)
    
))</errorConditionFormula>
        <errorMessage>CD-006. El estado del  candidato se encuentra como : Nuevo,Descartado,Verificando,Asignado , por favor no convertir.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CD007_Convertir_solo_estado_convertido</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
IsConverted = TRUE, 
OR( 
 Estado_del_candidato_1__c = &quot;Nuevo&quot;, 
 Estado_del_candidato_1__c= &quot;Verificando&quot;, 
 Estado_del_candidato_1__c = &quot;Descartado&quot;

))</errorConditionFormula>
        <errorMessage>CD-007.  El estado del  candidato se encuentra como : Nuevo,Descartado,Verificando, por favor no convertir.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CD008_No_convertir_candidato_NO_dueno</fullName>
        <active>true</active>
        <description>Si el candidato no es del propietario no dejarlo convertir</description>
        <errorConditionFormula>AND( 

OwnerId &lt;&gt; $User.Id ,

IsConverted = TRUE


)</errorConditionFormula>
        <errorMessage>CD-008. No es propietario de este lead, por favor no convertir.</errorMessage>
    </validationRules>
</CustomObject>
