    <validationRules>
        <fullName>Una_solicitud_aprobada_no_puede_ser_modi</fullName>
        <active>true</active>
        <errorConditionFormula>AND (
ISPICKVAL ( Estado_de_aprobacion_Riegos__c , &quot;Aprobado&quot;), 
 $UserRole.Id  &lt;&gt; &quot;00eo0000000TzBZ&quot;, 
OR (
   ISCHANGED( Name),
   ISCHANGED( Zona__c ),
   ISCHANGED( Ciudad__c ),
   ISCHANGED( Nit__c ),
   ISCHANGED( Direccion__c ),
   ISCHANGED( Telefono__c ),
   ISCHANGED( Fax__c ),
   ISCHANGED( E_mail__c ),
   ISCHANGED( Experiencia__c ),
   ISCHANGED( No_Viviendas_ultimos_5_anos__c ),
   ISCHANGED( Numero_de_viviendas_en_curso__c ),
   ISCHANGED( Solicitud_al_Gerente_Comercial__c ),
   ISCHANGED( Estado_de_aprobacion_Riegos__c ),
   ISCHANGED( Detalle_de_la_solicitud__c ),
   ISCHANGED( Observaciones__c ),
   ISCHANGED( Nombre_vendedor__c ),
   ISCHANGED( Representantel_legal__c ),
   ISCHANGED( Cedula_de_ciudadania__c ),
   ISCHANGED( Contacto_tramites__c ),
   ISCHANGED( Pagina_web__c )
))</errorConditionFormula>
        <errorMessage>CON-04 Una solicitud aprobada no puede ser modificada. Solo el Vicepresidente de Riesgos puede realizar esta acción.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Valida_actualizacion_solicitud_a_gerente</fullName>
        <active>true</active>
        <description>Valida que solo el gerente comercial pueda actualizar los valores devuelta y aprobado en el campo solicitud al gerente comercial</description>
        <errorConditionFormula>AND ( 
$Profile.Name &lt;&gt; &quot;Gerente Comercial&quot;, 

OR ( 
ISPICKVAL (  Solicitud_al_Gerente_Comercial__c , &quot;Aprobada&quot;), 
ISPICKVAL (  Solicitud_al_Gerente_Comercial__c, &quot;Devuelta&quot;) 
))</errorConditionFormula>
        <errorDisplayField>Solicitud_al_Gerente_Comercial__c</errorDisplayField>
        <errorMessage>CON-02  Con su perfil no puede aprobar o devolver solicitudes</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Valida_registro_de_observaciones</fullName>
        <active>true</active>
        <description>Cuando la solicitud se encuentra en Devuelta, se requiere el registro de la observación</description>
        <errorConditionFormula>AND ( 
 ISPICKVAL( Solicitud_al_Gerente_Comercial__c , &quot;Devuelta&quot;), 
   ISBLANK( Observaciones__c ))</errorConditionFormula>
        <errorDisplayField>Observaciones__c</errorDisplayField>
        <errorMessage>CON-01 Para registrar la solicitud con la opción devuelta, debe escribir una observación, explicando el motivo de la devolución.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Valida_registro_numero_Nit</fullName>
        <active>true</active>
        <description>Valida que le Nit sea  Numérico</description>
        <errorConditionFormula>NOT ISNUMBER(  Nit__c  )</errorConditionFormula>
        <errorDisplayField>Nit__c</errorDisplayField>
        <errorMessage>CON-03 El campo Nit es solo numérico</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
