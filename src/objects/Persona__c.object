    </validationRules>
    <validationRules>
        <fullName>Datos_obligatorios_Socio</fullName>
        <active>true</active>
        <errorConditionFormula>AND ( ISPICKVAL(Tipo__c, &apos;Socio&apos;), OR (  Pais_de_nacimiento__c  == &apos;&apos;,  Porcentaje_participacion__c == null))</errorConditionFormula>
        <errorMessage>Se deben ingresar los datos de Socio (País de Nacimiento y % de participación) para el tipo Socio</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>NIT_Numerico</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
 NIT_de_la_entidad__c != NULL,
 NOT( ISNUMBER(NIT_de_la_entidad__c) )
)</errorConditionFormula>
        <errorDisplayField>NIT_de_la_entidad__c</errorDisplayField>
        <errorMessage>El NIT de la entidad solamente puede contener números</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>No_personas_para_personas_naturales</fullName>
