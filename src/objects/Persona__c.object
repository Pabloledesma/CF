    <validationRules>
        <fullName>Datos_obligatorios_Representante_Legal</fullName>
        <active>true</active>
        <description>Se deben ingresar los datos de la empresa para el Representante Legal</description>
        <errorConditionFormula>AND ( ISPICKVAL(Tipo__c, &apos;Representante Legal&apos;), OR ( NIT_de_la_entidad__c == &apos;&apos;, Razon_social__c == &apos;&apos;))</errorConditionFormula>
