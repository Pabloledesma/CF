					<td class="s20">{!Cuenta.Tipo_Entidad__c}</td>
					<td class="s21" colspan="2">{!Cuenta.Entidad__c}</td>
					<td class="s20">{!Cuenta.Ciudad__c}</td>
					<td class="s20">{!Cuenta.Sucursal__c}</td>
