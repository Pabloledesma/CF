				<tr style="height: 19px; display: {!IF(Cuenta.Titulo__c != '', 'table-row', 'none')};">
					<td class="s13" colspan="10">{!Cuenta.Titulo__c}</td>
				</tr>
				<tr style='height: 30px;'>
					<td class="s19">{!Cuenta.Fecha_Corte__c}</td>
