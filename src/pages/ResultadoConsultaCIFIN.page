				<td class="s16">Estado</td>
				<td class="s16">Tipo ENT</td>
				<td class="s17" colspan="2">Entidad</td>
				<td class="s16">Ciudad</td>
				<td class="s16">Sucursal</td>
				<td class="s16">Fecha Apertura</td>
			</tr>
			<tr style='height: 32px;'>
				<td class="s16">Cupo sobregiro</td>
				<td class="s16">Dias autor</td>
				<td class="s16">Fecha permanencia</td>
				<td class="s16">Che Devueltos último mes</td>
				<td class="s18" colspan="6"></td>
			</tr>
			<tr style='height: 5px;'>
				<td class="s14" colspan="10"></td>
			</tr>
