					<td class="s9" colspan="3">{!Cartera.Tipo__c}</td>
					<td class="s20"></td>
					<td class="s20"></td>
					<td class="s20"></td>
					<td class="s20"></td>
					<td class="s20"></td>
					<td class="s20"></td>
				</tr>
